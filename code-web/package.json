{
  "name": "sample-application",
  "version": "0.0.1-SNAPSHOT",
  "private": true,
  "description": "Description for SampleApplication",
  "license": "UNLICENSED",
  "scripts": {
    "build": "npm run webapp:prod --",
    "build-watch": "concurrently 'npm run webapp:build:dev -- --watch' npm:backend:start",
    "clean-www": "rimraf dist/static/app/{src,target/}",
    "cleanup": "rimraf dist/",
    "e2e": "npm run e2e:protractor --",
    "e2e:dev": "concurrently -k -s  \"npm run e2e\"",
    "e2e:devserver": "concurrently -k -s  \"npm start\" \"wait-on http-get://localhost:9000 && npm run e2e:headless -- -c baseUrl=http://localhost:9000\"",
    "pree2e:headless": "npm run ci:server:await",
    "e2e:headless": "npm run e2e:protractor:headless --",
    "e2e:protractor": "protractor src/test/javascript/protractor.conf.js",
    "e2e:protractor:headless": "npm run e2e:protractor --",
    "e2e:update-webdriver": "webdriver-manager update --gecko false",
    "postinstall": "npm run e2e:update-webdriver",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "npm run lint -- --fix",
    "serve": "npm run start --",
    "start": "ng serve --hmr",
    "watch": "concurrently npm:start npm:backend:start",
    "webapp:build": "npm run clean-www && npm run webapp:build:dev",
    "webapp:build:dev": "ng build --configuration development",
    "webapp:build:prod": "ng build --configuration production",
    "webapp:dev": "ng serve",
    "webapp:dev-ssl": "ng serve --ssl",
    "webapp:dev-verbose": "ng serve --verbose",
    "webapp:prod": "npm run clean-www && npm run webapp:build:prod",
    "webapp:test": "npm run test --"
  },
  "config": {
    "backend_port": 8080,
    "default_environment": "prod",
    "packaging": "jar"
  },
  "dependencies": {
    "@angular/common": "13.3.1",
    "@angular/compiler": "13.3.1",
    "@angular/core": "13.3.1",
    "@angular/forms": "13.3.1",
    "@angular/localize": "13.3.1",
    "@angular/platform-browser": "13.3.1",
    "@angular/platform-browser-dynamic": "13.3.1",
    "@angular/router": "13.3.1",
    "@fortawesome/angular-fontawesome": "0.10.2",
    "@fortawesome/fontawesome-svg-core": "6.1.1",
    "@fortawesome/free-solid-svg-icons": "6.1.1",
    "@ng-bootstrap/ng-bootstrap": "12.0.1",
    "@ngx-translate/core": "14.0.0",
    "@ngx-translate/http-loader": "7.0.0",
    "@popperjs/core": "2.11.5",
    "bootstrap": "5.1.3",
    "dayjs": "1.11.0",
    "ngx-infinite-scroll": "13.0.1",
    "ngx-webstorage": "9.0.0",
    "rxjs": "7.5.5",
    "swagger-ui-dist": "4.2.1",
    "tslib": "2.3.1",
    "zone.js": "0.11.4"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "13.1.0",
    "@angular-builders/jest": "13.0.3",
    "@angular-devkit/build-angular": "^13.3.9",
    "@angular-eslint/eslint-plugin": "13.2.0",
    "@angular/cli": "13.3.1",
    "@angular/compiler-cli": "13.3.1",
    "@angular/service-worker": "13.3.1",
    "@types/chai": "4.2.14",
    "@types/chai-string": "1.4.2",
    "@types/jest": "27.4.1",
    "@types/mocha": "8.2.0",
    "@types/node": "16.11.26",
    "@types/selenium-webdriver": "4.0.11",
    "@typescript-eslint/eslint-plugin": "5.18.0",
    "@typescript-eslint/parser": "5.18.0",
    "browser-sync": "^2.27.10",
    "browser-sync-webpack-plugin": "2.3.0",
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "chai-string": "1.5.0",
    "concurrently": "7.1.0",
    "copy-webpack-plugin": "10.2.4",
    "eslint": "8.12.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-webpack-plugin": "3.1.1",
    "folder-hash": "4.0.2",
    "lint-staged": "12.3.7",
    "merge-jsons-webpack-plugin": "2.0.1",
    "prettier": "2.6.2",
    "prettier-plugin-java": "1.6.1",
    "prettier-plugin-packagejson": "2.2.17",
    "protractor": "7.0.0",
    "rimraf": "3.0.2",
    "ts-jest": "27.1.4",
    "ts-node": "9.1.1",
    "typescript": "4.6.3",
    "wait-on": "6.0.1",
    "webdriver-manager": "12.1.8",
    "webpack-bundle-analyzer": "4.5.0",
    "webpack-merge": "5.8.0",
    "webpack-notifier": "1.15.0"
  },
  "engines": {
    "node": ">=16.14.0"
  },
  "cacheDirectories": [
    "node_modules"
  ],
  "jestSonar": {
    "reportPath": "target/test-results/jest",
    "reportFile": "TESTS-results-sonar.xml"
  },
  "overrides": {
    "webpack": "5.71.0"
  }
}
